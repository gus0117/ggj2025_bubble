[resource]
script = ExtResource("1_jkths")
titulo = "Missing Cat"
nombre = "Martin Flores"
edad = 39
pacienciaActual = 0.0
coartada = "No sé nada del gato perdido. Ese día llegué al trabajo a las [b]8:30 AM[/b], como lo muestra el registro de mi pase de entrada. Salí a las [b]6:30 PM[/b], tomé el [b]autobús[/b] de regreso a casa y llegué a las 7:00 PM. No fui a ningún otro lugar. En cuanto a las [b]huellas en la cerca[/b], mi patio está al lado de un sendero donde suelen pasar gatos callejeros. La cerca fue pintada hace poco, y probablemente el gato las dejó mientras cruzaba, pero no entró a mi casa. No tengo nada que esconder."
urlPrueba1 = "Tarjeta"
p1_debil = "Un pase de empresa común."
p1_incompleta = "Un pase de empresa para poder entrar a las instalaciones con el [b]nombre del sospechoso y su información.[/b]"
p1_completa = "Un pase de empresa para entrar con el nombre del sospechoso, su información y su rango en la empresa parece que esta un [b]poco gastado[/b]."
urlPrueba2 = "Boleto"
p2_debil = "Un ticket de autobús usado, no tiene auto."
p2_incompleta = "Un ticket de autobús usado en la línea que [b]conecta su trabajo con su casa.[/b]"
p2_completa = "Un ticket de autobús con registro de compra a las [b]6:45 PM[/b], coincidiendo con la hora de salida de su trabajo, y que [b]conecta su lugar de trabajo con su casa.[/b]"
urlPrueba3 = "Huellas"
p3_debil = "La valla hace poco pintada, [b]tiene algunas imperfecciones.[/b]"
p3_incompleta = "Hay imperfecciones en la valla [b]parecen[/b] ser huellas de patas."
p3_completa = "[b]Huellas pequeñas[/b] y claramente definidas de felino en la pintura fresca de la cerca del sospechoso, que [b]no conducen al interior de su casa[/b] pero parecen seguir un sendero hacia el vecindario."
esVerdad = true
